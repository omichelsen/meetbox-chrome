{
  "manifest_version": 2,
  "name": "Meetbox",
  "description": "This extension allows Chrome to act as a Meetbox client.",
  "version": "1.0",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persitant": false
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Start Meetbox"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "tabs",
    "webNavigation",
    "https://*.g2m.me/*",
    "https://*.gotomeeting.com/*"
  ]
}